#include<stdio.h>

int check()
{
	int x = 1;
	return (*(char*)&x);
}//计算机数据排列模式

int main()
{
	if (check() == 1)
		printf("小端");
	else
		printf("大端");
	return 0;
}

#include<stdio.h>
#include<string.h>
int main()
{
	int i = 0;
	char a[1000] = { 0 };
	for ( i = 0; i < 1000; i++)
	{
		a[i] = -1 - i;
	}
	printf("%d\n", strlen(a));
	return 0;
}

#include<stdio.h>
int main()
{
	int n = 13;
	float*p = (float*)&n;
	printf("%d %f\n", n, *p);
	*p = 13.0;//(-1)^0*1.101*2^3
	printf("%d %f", n, *p);
	return 0;
}
