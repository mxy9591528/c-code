#include<stdio.h>
int main()
{
	int a[10] = { 0 };
	int d = 10, b = 12, c = 13;
	void*arr[3] = {&d,&b,&c };
	for (int i = 0; i < 3; i++)
	{
		printf("%p ", arr[i]);
	}
	int *pa = a;
  int**ppa = &pa;//二级指针
	printf("%p %p\n", &ppa, &pa);
	printf("%p\n", a);
	printf("%p\n", a[0]);
	printf("%p\n", a+1);
	printf("%p\n", &a);
	printf("%p\n", &a[0]+1);
	printf("%p\n", &a+1);
	return 0;
}
