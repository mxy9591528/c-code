#include<stdio.h>
#include<stdlib.h>
#include<errno.h>
#include<string.h>
int main()
{
	int i = 0;
	int n = 0;
	scanf("%d", &n);
	int*p = (int*)malloc(LLONG_MAX); //n * sizeof(int)
	if (!p)
		printf("%s\n", strerror(errno));
	else
	{
		for (i = 0; i < n; i++)
		{
			*(p + i) = i;
			printf("%d ", *(p + i));
		}
	}
	free(p);
	p = NULL;//置为空指针，保证数据安全
	return 0;
}
