#include<stdio.h>
#include<time.h>
#include<stdlib.h>

void reverse(int*a,int n )
{
	int left = 0;
	int right =n;
	int temp;
	while (left < right)
	{
		temp = a[left];
		a[left] = a[right];
		a[right] = temp;
		left++;
		right--;
	}
}
int main()
{
	int a[256] = { 0 };
	srand((unsigned int)time(NULL));
	for (int i = 0; i < 256; i++)
	{
		a[i] = rand();
	}
	reverse(a, 256);
	for (int i = 0; i < 256; i++)
	{
		printf("%d ", a[i]);
	}
	return 0;
}
